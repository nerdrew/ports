<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd" >
<plist version='1.0'>
<dict>
<key>Label</key><string>org.macports.postgresql84-server</string>
<key>UserName</key>
<string>postgres</string>
<key>GroupName</key>
<string>postgres</string>

<key>EnvironmentVariables</key>
<dict>
  <key>PGDATA</key>
  <string>/opt/local/var/db/postgresql/defaultdb84</string>
</dict>

<key>ProgramArguments</key>
<array>
  <string>/opt/local/lib/postgresql84/bin/postmaster</string>
</array>

<key>StandardOutPath</key>
<string>/opt/local/var/db/postgresql/logfile84</string>

<key>StandardErrorPath</key>
<string>/opt/local/var/db/postgresql/logfile84</string>

<key>Debug</key><false/>
<key>Disabled</key><true/>
<key>OnDemand</key><false/>
<key>RunAtLoad</key><false/>
</dict>
</plist>
