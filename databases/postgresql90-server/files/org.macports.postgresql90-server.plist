<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd" >
<plist version='1.0'>
<dict>
<key>Label</key><string>org.macports.postgresql90-server</string>
<key>UserName</key>
<string>postgresql</string>
<key>GroupName</key>
<string>postgresql</string>

<key>EnvironmentVariables</key>
<dict>
  <key>PGDATA</key>
  <string>/opt/local/var/db/postgresql/defaultdb90</string>
</dict>

<key>Program</key>
<string>/opt/local/lib/postgresql90/bin/postmaster</string>

<key>ProgramArguments</key>
<array>
</array>

<key>StandardOutPath</key>
<string>/opt/local/var/log/postgresql/postgresql90.log</string>

<key>StandardErrorPath</key>
<string>/opt/local/var/log/postgresql/postgresql90.log</string>

<key>Debug</key><false/>
<key>Disabled</key><true/>
<key>OnDemand</key><false/>
<key>RunAtLoad</key><false/>
</dict>
</plist>
